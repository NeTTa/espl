int main (int argc, char* argv[]){
  
  char *firstString, *secondString;
  firstString = argv[1];
  secondString = argv[2];
  int whichStrIsBigger = 0;
  whichStrIsBigger = hsvmhr(firstString, secondString);
  
  if (whichStrIsBigger>0)
    printf("the first string %s is BIGGER then the second string %s", firstString, secondString);
  else if (whichStrIsBigger<0)
    printf("the first string %s is SMALLER then the second string %s", firstString, secondString);
  else printf("HOORAY!\tthe two strings are identical");
  
  returnt 0;
}